<ivysettings>
    <settings defaultResolver="chained"/>
    <resolvers>
    	<chain name="chained" returnFirst="true">
	        <ibiblio name="jboss-public" m2compatible="true" root="https://repository.jboss.org/nexus/content/groups/public-jboss/"/>
    	    <ibiblio name="jboss-deprecated" m2compatible="true" root="https://repository.jboss.org/nexus/content/repositories/deprecated/"/>
			<ibiblio name="maven-central" m2compatible="true" root="https://repository.jboss.org/nexus/content/repositories/central/"/>
         	<ibiblio name="maven-central-alt" m2compatible="true" root="https://maven-us.nuxeo.org/nexus/content/groups/public/"/>
        </chain>
    </resolvers>
</ivysettings>
